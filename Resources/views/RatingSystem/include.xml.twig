{# purpose of this template: rating systems xml inclusion template #}
<ratingsystem id="{{ ratingSystem.getId() }}" createdon="{{ ratingSystem.createdDate|localizeddate('medium', 'short') }}" updatedon="{{ ratingSystem.updatedDate|localizeddate('medium', 'short') }}">
    <id>{{ ratingSystem.id }}</id>
    <scaleDim>{{ ratingSystem.scaleDim }}</scaleDim>
    <iconFa><![CDATA[{{ ratingSystem.iconFa }}]]></iconFa>
    <iconUrl>{{ ratingSystem.iconUrl }}</iconUrl>
</ratingsystem>
