{
    "name": "paustian/ratings-module",
    "version": "3.0.0",
    "description": "A  hook provider module that will attach ratings to any hookable subscriber module that implements the uiHookSubscribers. ",
    "type": "zikula-module",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Timothy Paustian",
            "email": "tdpaustian@gmail.com",
            "homepage": "https://www.microbiologytext.com/",
            "role": "owner"
        }
    ],
    "autoload": {
        "psr-4": { "Paustian\\RatingsModule\\": "" }
    },
    "require": {
        "php": ">=5.5.9"
    },
    "require-dev": {
        "jakub-onderka/php-parallel-lint": "^1",
        "pdepend/pdepend": "^2",
        "phploc/phploc": "^4",
        "phpmd/phpmd": "^2",
        "phpunit/phpunit": "^5",
        "sebastian/phpcpd": "^3",
        "squizlabs/php_codesniffer": "^3",
        "theseer/phpdox": "^0"
    },
    "extra": {
        "zikula": {
            "core-compatibility": ">=2.0.11 <3.0.0",
            "class": "Paustian\\RatingsModule\\PaustianRatingsModule",
            "displayname": "Ratings",
            "url": "ratings",
            "capabilities": {
                "user": {"route": "paustianratingsmodule_rating_index"},
                "admin": {"route": "paustianratingsmodule_rating_adminindex"}
            },
            "securityschema": {
                "PaustianRatingsModule::": "::",
                "PaustianRatingsModule:RatingSystem:": "RatingSystem ID::",
                "PaustianRatingsModule:Rating:RatingSystem": "Rating ID:RatingSystem ID:",
                "PaustianRatingsModule:Rating:": "Rating ID::",
                "PaustianRatingsModule::Ajax": "::"
            }
        }
    },
    "config": {
        "vendor-dir": "vendor",
        "preferred-install": "dist",
        "optimize-autoloader": true,
        "platform": {
            "php": "7.0.30"
        },
        "sort-packages": true
    }
}
